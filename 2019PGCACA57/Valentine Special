#include <iostream>
#include<stdio.h>
using namespace std;

int main() {
long	int num;
string s;
int k,c=0;
	cin >> num>>k;
	cin>>s;
	
	for(int i=0;i<num&&c<k;i++)
	{
	    if(i==0&&s[i]!='1')
	    {s[i]='1';
	    c++;
	    }
	    else if(s[i]!='0'&&i!=0)
	    {
	        s[i]='0';
	        c++;
	    }
	   
	   
	}

	cout<<s;
}
